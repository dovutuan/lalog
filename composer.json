{
    "version": "1.0.0",
    "name": "dovutuan/lalog",
    "description": "Logging query laravel",
    "keywords": [
        "laravel",
        "logging query",
        "query"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dovutuan\\Lalog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dovutuan\\Lalog\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "dovutuan",
            "email": "tuando.0598@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "homepage": "https://github.com/dovutuan/lalog",
    "extra": {
        "laravel": {
            "providers": [
                "Dovutuan\\Lalog\\ServiceProvider"
            ],
            "aliases": {
                "LaLog": "Dovutuan\\Lalog\\Facades\\Lalog"
            }
        }
    },
    "scripts": {
        "check-style": "phpcs -p --standard=psr12 src/",
        "fix-style": "phpcbf -p --standard=psr12 src/",
        "test": "vendor/bin/phpunit --testdox tests",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "support": {
        "email": "tuando.0598@gmail.com",
        "issues": "https://github.com/dovutuan/lalog/issues",
        "wiki": "https://github.com/dovutuan/lalog/wiki",
        "source": "https://github.com/dovutuan/lalog",
        "docs": "https://github.com/dovutuan/lalog"
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.13",
        "phpunit/phpunit": "^12.3",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0"
    }
}

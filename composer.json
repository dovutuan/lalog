{
  "version": "1.0.0",
  "name": "dovutuan/lalog",
  "description": "Logging query laravel",
  "keywords": [
    "laravel",
    "logging query",
    "query"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Dovutuan\\Lalog\\": "src/"
    }
  },
  "authors": [
    {
      "name": "dovutuan",
      "email": "tuando.0598@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/dovutuan/lalog",
  "extra": {
    "laravel": {
      "providers": [
        "Dovutuan\\Lalog\\ServiceProvider"
      ],
      "aliases": {
        "LaLog": "Dovutuan\\Lalog\\Facades\\Lalog"
      }
    }
  },
  "scripts": {
    "cs": "phpcs --standard=PSR12 src/",
    "cbf": "phpcbf --standard=PSR12 src/"
  },
  "support": {
    "email": "tuando.0598@gmail.com",
    "issues": "https://github.com/dovutuan/lalog/issues",
    "wiki": "https://github.com/dovutuan/lalog/wiki",
    "source": "https://github.com/dovutuan/lalog",
    "docs": "https://github.com/dovutuan/lalog"
  },
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.9"
  }
}
